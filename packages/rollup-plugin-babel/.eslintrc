{
	"extends": ["eslint:recommended", "prettier", "plugin:prettier/recommended"],
	"rules": {
		"no-cond-assign": [0]
	},
	"env": {
		"es6": true,
		"browser": true,
		"mocha": true,
		"node": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2018,
		"ecmaFeatures": {
			"spread": true
		}
	}
}
